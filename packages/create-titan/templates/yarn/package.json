{
  "name": "titanrepo-basic-yarn",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "titan run build",
    "dev": "titan run dev --parallel",
    "lint": "titan run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "titan": "latest"
  }
}
