{
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "titan run build",
    "clean": "titan run clean",
    "dev": "titan run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "titan run lint",
    "test": "titan run test"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "titan": "latest"
  }
}
